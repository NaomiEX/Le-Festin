{
  "expo": {
    "name": "Recipes App",
    "slug": "recipes-app",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "description": "This app was created for a Computer Science project. The app functions as a cookbook where you can view, favourite, and filter recipes.",
    "githubUrl": "https://github.com/NaomiEX/Recipes_App"
  }
}
